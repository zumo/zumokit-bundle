{
  "name": "zumo/zumokit-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle for Zumo's ZumoKit service.",
  "version": "1.0.1",
  "keywords": [
    "zumokit"
  ],
  "homepage": "https://developers.zumo.money/",
  "authors": [
    {
      "name": "Zumo",
      "homepage": "https://zumo.money/"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "psr/log": "^1.0.1",
    "symfony/config": "^2.7|^3.0|^4.0",
    "symfony/dependency-injection": "^2.7|^3.4.26|^4.0",
    "symfony/event-dispatcher": "^2.7|^3.0|^4.0",
    "symfony/framework-bundle": "^2.8 || ^3.4.26 || ^4.1.12",
    "symfony/security-bundle": "^2.8 || ^3.0 || ^4.0",
    "symfony/finder": "^2.7|^3.0|^4.0",
    "paragonie/random_compat": "^1 || ^2 || ^9",
    "lcobucci/jwt": "^3.2",
    "guzzlehttp/guzzle": "~6.2",
    "guzzlehttp/psr7": "^1.4",
    "monolog/monolog": "^1.24"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/debug": "^2.7|^3.0|^4.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "swiftmailer/swiftmailer": "^4.3 || ^5.0 || ^6.0",
    "symfony/console": "^2.8 || ^3.0 || ^4.0",
    "symfony/phpunit-bridge": "^2.8 || ^3.0 || ^4.0",
    "symfony/yaml": "^2.8 || ^3.0 || ^4.0",
    "lexik/jwt-authentication-bundle": "^2.6",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^4.1",
    "squizlabs/php_codesniffer": "^3.4",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "nyholm/symfony-bundle-test": "^1.4",
    "matthiasnoback/symfony-dependency-injection-test": "^3.1",
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Zumo\\ZumokitBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Zumo\\ZumokitBundle\\Tests\\": "/Tests/"
    }
  },
  "support": {
    "docs": "https://developers.zumo.money/"
  }
}
